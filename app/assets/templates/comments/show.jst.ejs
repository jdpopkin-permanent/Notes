<div class="comment">
  <div class="comment-author"><%= comment.user.username %> posted:</div>
  <%= JST["votes/div"]({
    totalVotes: comment.total_votes,
    currentUser: currentUser,
    votableType: "Comment",
    votableId: comment.id,
    voteSet: Notes.votes
  }) %>
  <div class="comment-body">
    <%= comment.body %>
  </div>
</div>